{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    // "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/src/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "app.d.ts",
    "global.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}